---
import Icon from '~/components/Icon.astro'

interface Props {
  filled?: boolean
}

let { filled } = Astro.props
---

<Icon class:list={[filled && 'filled']} name="star" size="xs" />

<style>
  .filled {
    fill: var(--color-content-primary);

    @media print {
      fill: var(--color-content-print);
    }
  }
</style>

---
import Date from '~/components/Date.astro'

interface Props {
  itemprop?: string
  date: Date
}

let { date, ...props } = Astro.props
---

<Date class="text" date={date} {...props} />

<style>
  .text {
    font: var(--font-xs);
    line-height: 1 !important;
    color: var(--color-content-primary);
  }
</style>

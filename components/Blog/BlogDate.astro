---
import BlogInfoElement from '~/components/Blog/BlogInfoElement.astro'
import Icon from '~/components/Icon.astro'
import Date from '~/components/Date.astro'

interface Props {
  itemprop?: string
  date: Date
}

let { date, ...props } = Astro.props
---

<BlogInfoElement>
  <Icon name="calendar" size="xs" />
  <Date {date} {...props} />
</BlogInfoElement>

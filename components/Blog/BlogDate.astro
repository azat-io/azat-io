---
import Typography from '~/components/Typography.astro'
import Date from '~/components/Date.astro'

interface Props {
  date: Date
}

let { date } = Astro.props
---

<Typography class="text" size="xs">
  <Date date={date} />
</Typography>

<style>
  .text {
    line-height: 1 !important;
  }
</style>

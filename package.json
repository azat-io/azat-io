    "typescript": "^5.9.3",
    "typograf": "^7.6.0",
    "unist-util-visit": "^5.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@azat-io/eslint-config": "^2.68.0",
